{
    "name": "pixelperfectat/active-campaign-api",
    "description": "PHP Active Campaign v3 API client",
    "license": "MIT",
    "keywords": [
        "api"
    ],
    "authors": [
        {
            "name": "Andr√© Flitsch",
            "email": "af@pixelperfect.at"
        }
    ],
    "require": {
        "php": "^7.0",
        "php-http/httplug": "^1.0",
        "php-http/multipart-stream-builder": "^1.0",
        "php-http/client-common": "^1.1",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "php-http/message-factory": "^1.0",
        "php-http/curl-client": "^1.6",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.3",
        "nyholm/nsa": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pixelperfect\\ActiveCampaign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pixelperfect\\ActiveCampaign\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
